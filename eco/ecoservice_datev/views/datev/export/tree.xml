<odoo>

  <record id="view_datev_export_tree" model="ir.ui.view">
    <field name="name">DATEV Export</field>
    <field name="model">datev.export</field>
    <field name="arch" type="xml">

      <tree
        decoration-success="generated==True"
        decoration-danger="generated==False"
      >
        <field name="generated" invisible="1"/>
        <field name="type"/>
        <field name="date_from"/>
        <field name="date_to"/>
        <field name="company_id"/>
      </tree>

    </field>
  </record>

</odoo>
